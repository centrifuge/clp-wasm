{
  "name": "clp-wasm",
  "version": "0.0.1",
  "description": "A CLP wrapper for WASM",
  "main": "dist/index.js",
  "author": "Darien Pardinas",
  "license": "ECLIPSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/dpar39/clp-wasm.git"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^25.5.1",
    "typescript": "^3.8.3"
  },
  "files": [
    "dist/solver.*"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build-docker": "docker build . -t clp-wasm-builder",
    "build-wasm": "docker run -u $(id -u):$(id -g) -v $PWD:/app clp-wasm-builder ./build.sh release wasm",
    "build": "rm -rf dist && npm run build-docker && npm run build-wasm",
    "start": "jest --config jestconfig.json --watchAll",
    "test": "jest --config jestconfig.json"
  },
  "dependencies": {},
  "peerDependencies": {
    "typescript": ">= 3.8.3"
  }
}
